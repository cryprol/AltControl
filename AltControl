getgenv().Settings = {
    Prefix = "."; -- Command Prefix
    FPS = 60; -- Optional [ Alt(s) Only ]
    Main = "MainAccount"; -- Main / Controller

    Instances = {
        ["Alt1"] = true,
        ["Alt2"] = true,
    };

    Whitescreen = false; -- Reduce GPU Usage
    Anonymous = false; -- Hides Usernames
};

loadstring(game:HttpGet("https://raw.githubusercontent.com/cryprol/AltControl/refs/heads/main/Main"))()

