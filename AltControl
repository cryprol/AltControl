_G.Settings = {
    Prefix = "."; -- Command Prefix
    FPS = 60; -- Optional [ Alt(s) Only / Unless FPSLock ]
    FPSLock = false; -- True = Lock Main FPS
    Main = "MainAccount"; -- Main / Controller [ Case Sensitive ]

    Instances = { -- [ Case Sensitive ]
        ["Alt1"] = true,
